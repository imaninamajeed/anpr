{
	"branches": [
		"main",
		"next",
		{
			"name": "beta",
			"prerelease": true
		}
	],
	"plugins": [
		[
			"@semantic-release/commit-analyzer",
			{
				"releaseRules": [
					{
						"type": "docs",
						"scope": "README",
						"release": "patch"
					},
					{
						"type": "refactor",
						"release": "patch"
					},
					{
						"scope": "no-release",
						"release": false
					}
				]
			}
		],
		[
			"@semantic-release/release-notes-generator",
			{
				"writerOpts": {
					"commitsSort": [
						"subject",
						"scope"
					]
				}
			}
		],
		[
			"@semantic-release/gitlab",
			{
				"gitlabUrl": "http://172.17.0.129"
			}
		]
	]
}